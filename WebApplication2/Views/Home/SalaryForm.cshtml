<div class='row'>
    <div class='col-xs-12 col-lg-6'>
        <div class='col-xs-12 col-lg-6'>
            <input type='text' class='form-control' placeholder='Month/Years' ng-model='salaryMonthYear' />
        </div>
        <div class='col-xs-12 col-lg-6'>
            <input type='text' class='form-control' placeholder='Amount' ng-model='salaryamt' />
        </div>
    </div>
    <div class='col-xs-12 col-lg-6'>
        <div class='col-xs-2'>
            <button type='button' class='btn btn-primary' ng-click='addSalary()'>Add</button>
        </div>
        <div class='col-xs-10'>
            <p class="text-right" style="font-size:medium">
                <strong ng-if="TotalSalaryIncome != 0">
                    RS.<span class="label label-warning" style="font-size:medium">{{TotalSalaryIncome}}</span>
                </strong>
            </p>
        </div>
    </div>
</div>
<div class='row'>&nbsp;</div>
<div class='row'>
    <div class='col-xs-12 col-lg-6'>
        <div class='col-xs-12'>
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <p class="panel-title">Salary History</p>
                </div>
                <div class="panel-body" style="height:300px;overflow-y:auto;">
                    <salary-list info="SalaryList"></salary-list>
                </div>
            </div>
        </div>
    </div>
    <div class='col-xs-12 col-lg-6'>
        <div class='col-xs-12'>
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <p class="panel-title">Salary Breakup</p>
                </div>
                <div class="panel-body" style="height:300px;overflow-y:auto;">
                    <salary-list info="SalaryListLocal"></salary-list>
                </div>
            </div>
        </div>
    </div>
</div>


